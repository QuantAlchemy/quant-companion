{
  "name": "quant-companion",
  "description": "A tool to help you better understand your trade performance",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md,html}\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky",
    "preinstall": "npx only-allow pnpm",
    "serve": "vite preview",
    "rebase:production": "git checkout production && git rebase main && git push && git checkout main"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,html}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.{json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@kobalte/core": "^0.13.6",
    "@ralphsmith80/solid-plotly.js": "^0.0.1",
    "@ralphsmith80/tsparticles-solid": "3.0.1-alpha.0",
    "@tsparticles/configs": "^3.5.0",
    "@tsparticles/engine": "^3.5.0",
    "@tsparticles/slim": "^3.5.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "lucide-solid": "^0.428.0",
    "papaparse": "^5.4.1",
    "plotly.js": "^2.35.2",
    "solid-js": "^1.8.22",
    "solid-motionone": "^1.0.0",
    "tailwind-merge": "^2.5.2",
    "tailwindcss": "^3.4.10",
    "tailwindcss-animate": "^1.0.7",
    "tsparticles": "^3.5.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/chrome": "^0.0.270",
    "@types/node": "^22.5.1",
    "@types/papaparse": "^5.3.14",
    "@types/plotly.js": "^2.33.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-html": "^8.1.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-solid": "^0.14.2",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.9",
    "postcss": "^8.4.42",
    "prettier": "^3.3.3",
    "typescript": "^4.9.5",
    "typescript-eslint": "^8.3.0",
    "vite": "^4.5.3",
    "vite-plugin-solid": "^2.10.2"
  }
}
